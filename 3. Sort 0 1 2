#include <bits/stdc++.h> 
void sort012(int *arr, int n)
{
   int i,a=0,b=n-1;
   int arr1[n];
   
   for(i=0;i<n;i++){
      arr1[i]=arr[i];
      arr[i]=1;
   }
   for(i=0;i<n;i++){
      if(arr1[i]==0){
           arr[a]=0;
           a++;
      } else if (arr1[i] == 2) {
        arr[b]=2;
        b--;
      }
   }

}

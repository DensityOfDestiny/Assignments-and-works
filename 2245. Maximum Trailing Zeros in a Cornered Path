class Solution {
public:
    int trailingZeroes(int n) {
        int i,j;
        int z_count=0;

        for(i=0;i<=n;i=i=i+5){
            j=i;
            while(j>0 && (j%5==0 || j%10==0)){
            if(j%10 == 0){
                z_count++;
                j=j/10;
            }
            if(j%5==0){
                z_count++;
                j=j/5;
            }
            }
        }
        
            return z_count;
        
        
    }
};

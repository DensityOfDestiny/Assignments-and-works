#include <bits/stdc++.h>
vector<int> findArraySum(vector<int> &a, int n, vector<int> &b, int m) {
  int num1 = 0, num2 = 0;
  int sum = 0;
  int i, j;

  vector<int> c,d;

  for (i = 0; i<n; i++) {
    num1 = (num1 * 10) + a[i]; 
  }

  for(j=0;j<m;j++){
	num2 = (num2 * 10) + b[j];
  }
  sum = num1 + num2;
  

  while (sum > 0) {
    c.push_back(sum % 10);
    sum = sum / 10;
  }
  for(i=c.size()-1; i>=0; i--){
	  d.push_back(c[i]);
  }
  

  return d;
}

int findUnique(int *arr, int size)
{
    int i,j;
    int count=0;
    for(i=0; i<size; i++){
        for(j=0; j<size; j++){
            if(i!=j){
            if ((arr[i] ^ arr[j]) == 0) {
              break;
            }
        }
        }
        if(j==size){
            return arr[i];
        }
    }

   
}

class Solution {
public:
    int numIdenticalPairs(vector<int>& nums) {
        int count=0;
        int i,j;
        for(i=0;i<nums.size();i++)
            {
                for(j=0;j<nums.size();j++){
                    if (i<j){
                        if(nums[i]==nums[j]){
                            count++;
                        }
                    }
                }

            }
            return count;
        }
        
    
};

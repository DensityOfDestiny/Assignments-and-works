#include <bits/stdc++.h>
#include <iostream>
using namespace std;

int main() {
  int i, n, x;
  cin >> n;
  vector<int> arr(n, 0);
  for (i = 0; i < n; i++) {
    cin >> arr[i];
  }
  cin >> x;

  for (i = n - 1; i >= 0; i--) {
    if (arr[i] == x) {
      cout << i;
      break;
    }
  }
  if (i == -1) {

    cout << -1;
  }
}

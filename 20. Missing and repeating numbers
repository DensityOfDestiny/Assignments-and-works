#include <bits/stdc++.h>

pair<int, int> missingAndRepeating(vector<int> &arr, int n) {
  int count, i, j;
  int dup = -1, mis = -1;
  int num = 1;
  for (i = 0; i < n; i++) {
    count = 0;
    for (j = 0; j < n; j++) {
      if (arr[j] == num) {
        count++;
      }
    }
    if (count == 0) {
      mis = num;
    } else if (count == 2) {
      dup = num;
    }
    num++;
  }
  return make_pair(mis, dup);
}

class Solution {
public:
    int searchInsert(vector<int>& nums, int target) {
        int pos=-1;
        int i,j;
        for(i=0;i<nums.size();i++){
            if(nums[i]==target){
                pos=i;
                break;
            }
        }
        if(pos!=-1){
            return pos;
        }
        else{
            
            for(i=0;i<nums.size();i++){
                if(!(nums[i]<target)){
                    break;
                }


            }
            pos=i;
            return pos;

        }
    }
};

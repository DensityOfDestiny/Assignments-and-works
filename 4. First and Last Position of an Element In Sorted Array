#include <bits/stdc++.h> 
pair<int, int> firstAndLastPosition(vector<int>& arr, int n, int k)
{
    int i;
    int first=-1,last=-1;

    for(i=0;i<n;i++){

        if( arr[i] == k){
          if (first != -1) {
            last=i;
          }
          else{
              first=i;
              last=i;
          }
        }
    }
    return {first,last};
}

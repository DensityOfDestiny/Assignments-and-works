#include <bits/stdc++.h>
int specialSum(vector<int> &arr, int n) {
  int i, sum = 0, s = 0;
  for (i = 0; i < n; i++) {
    sum += arr[i];
  }

  while (sum > 0) {
    s += sum % 10;
    sum /= 10;
  }
  if (s > 9) {
    sum = s;
    s = 0;
    while (sum > 0) {
      s += sum % 10;
      sum /= 10;
    }
  }
  return s;
}

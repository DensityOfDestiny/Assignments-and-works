#include<iostream>
using namespace std;
int main () {
    int n, rev=0, rem;
    cout<<"Enter a number : ";
    cin>>n;
    cout<<"\nThe Entered number is : "<<n;
    while(n>0) {
        rem=n%10;
        rev=rev*10+rem;
        n=n/10;
    }
    cout<<"\nThe Reversed number is : "<<rev<<"\n";
    return 0;
}
